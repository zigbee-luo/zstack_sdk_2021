/*
 *  ======== ti_154stack_toad_config.h.xdt ========
 */

// Map the module properties to an array
const modules = Object.getOwnPropertyNames(system.modules);

// Get the first stack that includes the TOAD module
const toadModulePath = modules.find((path) => {
    return path.includes("ti154stack_toad_config");
});

const toadModule = system.modules[toadModulePath].$instances[0];
