% if(toadModule.enabled) {
/* Number of bytes in the Turbo OAD block cache */
#define TOAD_BLOCK_CACHE_SIZE       `toadModule.blockCacheSize`

/* Number of bytes in the Turbo OAD flash write buffer */
#define TOAD_WRITE_BUFFER_SIZE      `toadModule.writeBufferSize`
